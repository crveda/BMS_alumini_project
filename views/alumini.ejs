<body>
  <div class="container">
    <div class="navBar">
        <div class="logoBox">
            <div class="logo_img">
                <img src="../images/logo.png">
            </div>
            <div class="text_box">
                <h4>B.M.S ALUMINI NETWORK</h4>
                <p>Autonoumous Institute, Affiliated to VTU</p>
                <p>Estd, 1946</p>
            </div>
        </div>
        <div class="navLinkBox">
            <ul>
                <li><a href="/home">HOME</a></li>
                <li><a href="/events">EVENTS</a></li>
                <li><a href="/gallery">GALLERY</a></li>
                <li><a href="/alumini">ALUMINI'S</a></li>
                <li><a href="/News_feed">NEWSLETTER</a></li>
                <% if(isAuth) { %>
                    <li><a href="/logOut">LOGOUT</a></li>
                <% } else { %>
                  <li><a href="/join">JOIN</a></li>
                  <% } %>
              </ul> 
        </div>
    </div>


    <h1>Notable Alumini's</h1>
  <div class="row">
    <% for(let person in personListNotableAlumini ) { %>
        <div class="col-12 col-sm-6 col-md-4 col-lg-3">
        <div class="our-team">
            <div class="picture">
            <img class="img-fluid" src="<%= person.profilePic %>">
            </div>
            <div class="team-content">
            <h3 class="name"><%= person.fullName %></h3>
            <h4 class="title"><%= person.occupation%></h4>
            </div>
            <ul class="social">
            <li><a href="<%= person.facebookLink %>" class="fa fa-facebook" aria-hidden="true"></a></li>
            <li><a href="<%= person.twitterLink%>" class="fa fa-twitter" aria-hidden="true"></a></li>
            <li><a href="<%= person.googlePlusLink %>" class="fa fa-google-plus" aria-hidden="true"></a></li>
            <li><a href="<%= person.linkedinLink%>" class="fa fa-linkedin" aria-hidden="true"></a></li>
            </ul>
        </div>
        </div>
    <% } %>

  </div>
  
  
